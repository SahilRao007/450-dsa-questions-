int makeBeautiful(string s) {
	int c1=0;//case where even 1 and odd 0
    int c2=0;//case where even 0 and odd 1
    int n=s.size();
    for(int i=0;i<n;i++)
    {
        if(i%2==0 and s[i]!='1')
            c1++;
        if(i%2==0 and s[i]!='0')
            c2++;
        if(i%2!=0 and s[i]!='0')
            c1++;
        if(i%2!=0 and s[i]!='1')
            c2++;
    }
    return min(c1,c2);
}
