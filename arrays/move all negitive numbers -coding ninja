@1 mehtod can be use by sorting technique 
#include <bits/stdc++.h> 
vector<int> separateNegativeAndPositive(vector<int> &nums){
      int k=0;
    for(int i=0;i<nums.size();i++)
    {
        if(nums[i]<0)
         {    if(i!=k)
            swap(nums[i],nums[k]);
            k++;            
        }
       }
    return nums;
}

